<div class="container mt-3">
  <div class="mb-3">
  <h1>Add a new list</h1>
  <%= simple_form_for @list do |f| %>
      <%= f.input :name, placeholder: 'Drama' %>
      <%= f.input :photo, as: :file %>
      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
  </div>
  <%= link_to 'Back', lists_path, class: 'nav-link'%>
</div>
